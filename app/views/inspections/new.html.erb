Create New Inspection

<%= form_with model: @inspection do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :sheet_url_string %>
  <%= form.select :sheet_url_string, @sheets_selection %>
  <%= form.label :active %>
  <%= form.checkbox :active %>
  <%= form.submit "Save" %>
<% end %>