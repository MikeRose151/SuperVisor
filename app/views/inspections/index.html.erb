<h1>Inspections</h1>

<%= link_to "Create New Inspection", new_inspection_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Sheet url</th>
    <th>Created by ID</th>
    <th>Active</th>
  </tr>
  <% @inspections.each do |inspection| %> 
    <tr>
      <td> <%= inspection.title %> </td>
      <td> <%= inspection.sheet_url_string %> </td>
      <td> <%= inspection.created_by_id %> </td>
      <td> <%= inspection.active %> </td>
    </tr>
  <% end %>
</table>
